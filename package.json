{
  "name": "csss-ui",
  "version": "0.1.0",
  "description": "A vue3.0 components library based on csss",
  "main": "./dist/csss-ui.umd.js",
  "module": "./dist/csss-ui.es.js",
  "typings": "dist/index.d.ts",
  "author": "hansuhhi",
  "license": "MIT",
  "homepage": "https://hansuhhi.github.io/csss-ui/en-US/",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "cz": "git-cz",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "vite build && vue-tsc --noEmit && pnpm gen:types && pnpm lint:dist ",
    "build:test": "pnpm build && pnpm publish:test",
    "lint:eslint": "eslint **/*.{vue,ts,tsx} --fix",
    "lint:stylelint": "stylelint **/*.{vue,css} --fix",
    "lint:dist": "prettier ./dist/**/*.d.ts --write",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "gen:components": "esno src/scripts/gen-components.ts",
    "gen:types": "esno src/scripts/gen-types",
    "publish:test": "npm publish --registry http://localhost:4873/"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "dependencies": {
    "@hansuhhi-don/csss": "^0.0.0",
    "lodash-es": "^4.17.21",
    "titleize": "^3.0.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.2",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@hansuhhi-don/eslint-config": "^0.3.0",
    "@hansuhhi-don/stylelint-config": "^0.2.0",
    "@types/klaw-sync": "^6.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vitest/coverage-c8": "^0.25.3",
    "@vue/test-utils": "^2.2.4",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "esno": "^0.16.3",
    "flexsearch": "^0.7.31",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "klaw-sync": "^6.0.0",
    "markdown-it": "^13.0.1",
    "prettier": "^2.8.0",
    "rollup-plugin-delete": "^2.0.0",
    "stylelint": "^14.15.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-vitepress-demo": "2.0.0-alpha.7",
    "vitepress": "1.0.0-alpha.30",
    "vitepress-plugin-search": "1.0.4-alpha.15",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.9"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/csss-ui.es.js",
      "require": "./dist/csss-ui.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HanSuhhi/csss-ui"
  },
  "keywords": [
    "csss-ui",
    "component library",
    "headless",
    "vue"
  ]
}